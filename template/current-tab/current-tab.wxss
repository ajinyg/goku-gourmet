.current-model{
	width:750rpx;
	height:550rpx;
	/*background:rgba(255,255,255,.93);*/
	overflow-y:scroll;
	padding-bottom:30rpx;
}
.current-model.active{
	display:none;
}
.current-item{
	width:660rpx;
	height:80rpx;
	line-height:80rpx;
	margin:0 50rpx 0 50rpx;
	border-bottom:1rpx solid #E7E7E7;
	font-size:30rpx;
	color: #484848;
	font-family: PingFangTC-Regular;
}
.current-item.active{
	color:#F1712D;
	background:url(data:image/png;base64,) right no-repeat;
	background-size:9%;
}
.current-tab-list{
	display:none;
}
.current-tab-list.active{
	display:block;
}