<block wx:if="{{hiddenLoading}}">
	<loading >正在加载中</loading>  
</block>
<block wx:else>
	<scroll-view scroll-y="true" > 
			<view class="setup-con"> 
				<view class="setup-t"></view>
				<view class="setup-row" hover-class="button-hover" bindtap="get_currency" >币种</view>
				<view class="setup-row" hover-class="button-hover" bindtap="get_privacypolicy">服务条款</view>
				<!-- <view class="setup-row" hover-class="button-hover" bindtap="get_terms">隐私政策</view> -->
				<button class="setup-row" hover-class="button-hover" open-type="contact">联系客服</button>
				<view class="setup-row" hover-class="button-hover" bindtap="get_aboutus">关于我们</view>
			</view>
	</scroll-view>
</block>
