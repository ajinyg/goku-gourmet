	background:url('http://img08.oneniceapp.com/upload/resource/beb1c43883261b15b8d40685002a5317.png') center no-repeat;
	background-size:100%;
}
.ad-con-item .ad-txt{
	margin:0 auto;
	padding:28rpx 0;
	width:173rpx;
	height:auto;
	font-size: 28rpx;
