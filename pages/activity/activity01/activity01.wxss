.dis-table{
	display:table;
	margin:0 auto;
}
.advantage-con{
	display:flex;
	margin:20rpx auto 0 auto;
}
.ad-con-item{
	width:173rpx;
	height:310rpx;
	margin-right:45rpx;
	border-radius:10rpx;
}
.ad-con-item .ad-icon{
	margin:21rpx auto 0 auto;
